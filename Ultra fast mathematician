#include<bits/stdc++.h>
using namespace std;
int main(void)
{
    int t;
    cin>>t;
    while(t--)
    {
        string a,b,res;
        cin>>a>>b;
        for(int i=0;i<a.length();i++)
        {
            if(a[i]==b[i])
            {
                res+='0';
            }
            else
                res+='1';
        }
        cout<<res<<endl;
    }
}
